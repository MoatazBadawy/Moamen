<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fadeScrollbars="false"
    android:fastScrollEnabled="false"
    android:overScrollMode="never"
    android:scrollbars="none"
    tools:context=".Fragment.ImagesFragmentProject.CategoryImagesFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <!-- LinearLayout 1 -->
        <LinearLayout
            android:id="@+id/linear_layout_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:orientation="horizontal"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <Button
                android:id="@+id/button_ertugrel"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:layout_marginRight="4dp"
                android:layout_weight="1"
                android:background="@drawable/ertugrel" />

            <Button
                android:id="@+id/button_ma3alem_islamic"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:layout_marginLeft="4dp"
                android:layout_marginRight="6dp"
                android:layout_weight="1"
                android:background="@drawable/ma3alem_islamic" />

        </LinearLayout>

        <!-- LinearLayout text -->
        <LinearLayout
            android:id="@+id/linear_layout_text"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:orientation="horizontal"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linear_layout_1">

            <TextView
                android:id="@+id/textView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="@+id/textView"
                android:layout_marginStart="8dp"
                android:text="قيامة أرطغرل"
                android:textColor="#000"
                android:textSize="18sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="@+id/textView"
                android:layout_marginRight="92dp"
                android:text="معالم إسلامية"
                android:textColor="#000"
                android:textSize="18sp"
                android:textStyle="bold" />
        </LinearLayout>

        <!-- LinearLayout 2 -->
        <LinearLayout
            android:id="@+id/linear_layout_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="48dp"
            android:orientation="horizontal"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linear_layout_1">

            <Button
                android:id="@+id/button1"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:layout_marginRight="4dp"
                android:layout_weight="1"
                android:background="@drawable/aqwal_image" />

            <Button
                android:id="@+id/button_2"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:layout_marginLeft="4dp"
                android:layout_marginRight="6dp"
                android:layout_weight="1"
                android:background="@drawable/more_image" />

        </LinearLayout>

        <!-- LinearLayout text -->
        <LinearLayout
            android:id="@+id/linear_layout_text2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:orientation="horizontal"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linear_layout_2">

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="@+id/textView"
                android:layout_marginStart="8dp"
                android:text="أقوال الصالحين"
                android:textColor="#000"
                android:textSize="18sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="@+id/textView"
                android:layout_marginRight="82dp"
                android:text="صور منوعة"
                android:textColor="#000"
                android:textSize="18sp"
                android:textStyle="bold" />
        </LinearLayout>

        <!-- LinearLayout 3 -->
        <LinearLayout
            android:id="@+id/linear_layout_3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="48dp"
            android:orientation="horizontal"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linear_layout_2">

            <Button
                android:id="@+id/button3"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:layout_marginRight="4dp"
                android:layout_weight="1"
                android:background="@drawable/morning_image" />

            <Button
                android:id="@+id/button_4"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:layout_marginLeft="4dp"
                android:layout_marginRight="6dp"
                android:layout_weight="1"
                android:background="@drawable/dowaa_rounder" />

        </LinearLayout>

        <!-- LinearLayout text -->
        <LinearLayout
            android:id="@+id/linear_layout_text3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:orientation="horizontal"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linear_layout_3">

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="@+id/textView"
                android:layout_marginStart="8dp"
                android:text="الصباح والمساء"
                android:textColor="#000"
                android:textSize="18sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="@+id/textView"
                android:layout_marginRight="82dp"
                android:text="صور أدعية"
                android:textColor="#000"
                android:textSize="18sp"
                android:textStyle="bold" />
        </LinearLayout>

        <!-- LinearLayout 4 -->
        <LinearLayout
            android:id="@+id/linear_layout_4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="48dp"
            android:orientation="horizontal"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linear_layout_3">

            <Button
                android:id="@+id/button4"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:layout_marginRight="4dp"
                android:layout_weight="1"
                android:background="@drawable/aqsa_image" />

            <Button
                android:id="@+id/button_5"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:layout_marginLeft="4dp"
                android:layout_marginRight="6dp"
                android:layout_weight="1"
                android:background="@drawable/madena_image" />

        </LinearLayout>

        <!-- LinearLayout text -->
        <LinearLayout
            android:id="@+id/linear_layout_text4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:orientation="horizontal"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linear_layout_4">

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="@+id/textView"
                android:layout_marginStart="8dp"
                android:text="المسجد الأقصى"
                android:textColor="#000"
                android:textSize="18sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="@+id/textView"
                android:layout_marginRight="82dp"
                android:text="المسجد النبوي"
                android:textColor="#000"
                android:textSize="18sp"
                android:textStyle="bold" />
        </LinearLayout>

        <!-- LinearLayout 5 -->
        <LinearLayout
            android:id="@+id/linear_layout_5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="48dp"
            android:orientation="horizontal"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linear_layout_4">

            <Button
                android:id="@+id/button6"
                android:layout_width="0dp"
                android:layout_height="98dp"
                android:layout_marginRight="4dp"
                android:layout_weight="1"
                android:background="@drawable/zahraa2" />

            <Button
                android:id="@+id/button_7"
                android:layout_width="0dp"
                android:layout_height="98dp"
                android:layout_marginLeft="4dp"
                android:layout_marginRight="6dp"
                android:layout_weight="1"
                android:background="@drawable/ramadan_image" />

        </LinearLayout>

        <!-- LinearLayout text -->
        <LinearLayout
            android:id="@+id/linear_layout_text5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linear_layout_5">

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="@+id/textView"
                android:layout_marginStart="10dp"
                android:text="زهرة"
                android:textColor="#000"
                android:textSize="18sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="@+id/textView"
                android:layout_marginRight="154dp"
                android:text="رمضان كريم"
                android:textColor="#000"
                android:textSize="18sp"
                android:textStyle="bold" />
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.core.widget.NestedScrollView>