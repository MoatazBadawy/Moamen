<?xml version="1.0" encoding="utf-8"?>

<androidx.core.widget.NestedScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fadeScrollbars="false"
    android:fastScrollEnabled="false"
    android:overScrollMode="never"
    android:scrollbars="none"
    tools:context=".Fragment.Images.CategoryImagesFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <!-- LinearLayout 1 -->
        <LinearLayout
            android:id="@+id/linear_layout_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:orientation="horizontal"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <Button
                android:id="@+id/button_ertugrel_category"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:layout_marginRight="4dp"
                android:layout_weight="1"
                android:background="@drawable/folder_ertugrel" />

            <Button
                android:id="@+id/button_islam_category"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:layout_marginLeft="4dp"
                android:layout_marginRight="6dp"
                android:layout_weight="1"
                android:background="@drawable/folder_ma3alem_islamic" />

        </LinearLayout>

        <!-- LinearLayout text -->
        <LinearLayout
            android:id="@+id/linear_layout_text"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:orientation="horizontal"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linear_layout_1">

            <TextView
                android:id="@+id/textView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:layout_alignParentBottom="@+id/textView"
                android:layout_marginStart="42dp"
                android:text="قيامة أرطغرل"
                android:textColor="#000"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="@+id/textView"
                android:layout_marginRight="106dp"
                android:text="معالم إسلامية"
                android:textStyle="bold"
                android:textColor="#000"
                android:textSize="16sp" />
        </LinearLayout>

        <!-- LinearLayout 2 -->
        <LinearLayout
            android:id="@+id/linear_layout_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="16dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linear_layout_text">

            <Button
                android:id="@+id/button_saleheen_category"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:layout_marginRight="4dp"
                android:layout_weight="1"
                android:background="@drawable/folder_aqwal_image" />

            <Button
                android:id="@+id/button_others_category"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:layout_marginLeft="4dp"
                android:layout_marginRight="6dp"
                android:layout_weight="1"
                android:background="@drawable/folder_more_image" />

        </LinearLayout>

        <!-- LinearLayout text -->
        <LinearLayout
            android:id="@+id/linear_layout_text2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:orientation="horizontal"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linear_layout_2">

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="@+id/textView"
                android:layout_marginStart="42dp"
                android:text="أقوال الصالحين"
                android:textStyle="bold"
                android:textColor="#000"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="@+id/textView"
                android:layout_marginRight="100dp"
                android:text="صور منوعة"
                android:textStyle="bold"
                android:textColor="#000"
                android:textSize="16sp" />
        </LinearLayout>

        <!-- LinearLayout 3 -->
        <LinearLayout
            android:id="@+id/linear_layout_3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="16dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linear_layout_text2">

            <Button
                android:id="@+id/button_morning_category"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:layout_marginRight="4dp"
                android:layout_weight="1"
                android:background="@drawable/folder_morning_image" />

            <Button
                android:id="@+id/button_dowaa_category"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:layout_marginLeft="4dp"
                android:layout_marginRight="6dp"
                android:layout_weight="1"
                android:background="@drawable/folder_dowaa_rounder" />

        </LinearLayout>

        <!-- LinearLayout text -->
        <LinearLayout
            android:id="@+id/linear_layout_text3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:orientation="horizontal"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linear_layout_3">

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="@+id/textView"
                android:layout_marginStart="42dp"
                android:text="الصباح والمساء"
                android:textStyle="bold"
                android:textColor="#000"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="@+id/textView"
                android:layout_marginRight="104dp"
                android:text="صور أدعية"
                android:textStyle="bold"
                android:textColor="#000"
                android:textSize="16sp" />
        </LinearLayout>

        <!-- LinearLayout 4 -->
        <LinearLayout
            android:id="@+id/linear_layout_4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="16dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linear_layout_text3">

            <Button
                android:id="@+id/button_aqsa_category"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:layout_marginRight="4dp"
                android:layout_weight="1"
                android:background="@drawable/folder_aqsa_image" />

            <Button
                android:id="@+id/button_nabawe_category"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:layout_marginLeft="4dp"
                android:layout_marginRight="6dp"
                android:layout_weight="1"
                android:background="@drawable/folder_madena_image" />

        </LinearLayout>

        <!-- LinearLayout text -->
        <LinearLayout
            android:id="@+id/linear_layout_text4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:orientation="horizontal"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linear_layout_4">

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="@+id/textView"
                android:layout_marginStart="42dp"
                android:text="المسجد الأقصى"
                android:textStyle="bold"
                android:textColor="#000"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="@+id/textView"
                android:layout_marginRight="94dp"
                android:text="المسجد النبوي"
                android:textStyle="bold"
                android:textColor="#000"
                android:textSize="16sp" />
        </LinearLayout>

        <!-- LinearLayout 5 -->
        <LinearLayout
            android:id="@+id/linear_layout_5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="16dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linear_layout_text4">

            <Button
                android:id="@+id/button_zahrah_category"
                android:layout_width="0dp"
                android:layout_height="98dp"
                android:layout_marginRight="4dp"
                android:layout_weight="1"
                android:background="@drawable/folder_zahraa2" />

            <Button
                android:id="@+id/button_ramadan_category"
                android:layout_width="0dp"
                android:layout_height="98dp"
                android:layout_marginLeft="4dp"
                android:layout_marginRight="6dp"
                android:layout_weight="1"
                android:background="@drawable/folder_ramadan_image" />

        </LinearLayout>

        <!-- LinearLayout text -->
        <LinearLayout
            android:id="@+id/linear_layout_text5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:layout_marginBottom="12dp"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linear_layout_5">

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="@+id/textView"
                android:layout_marginStart="62dp"
                android:text="زهرة"
                android:textStyle="bold"
                android:textColor="#000"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="@+id/textView"
                android:layout_marginRight="140dp"
                android:text="رمضان كريم"
                android:textStyle="bold"
                android:textColor="#000"
                android:textSize="16sp" />
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.core.widget.NestedScrollView>