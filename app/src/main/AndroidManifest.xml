<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.picsapp.moamenapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.SET_WALLPAPER" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ActivityProjects.ActivityDisplayImages.ImagesMorningActivity"></activity>
        <activity android:name=".ActivityProjects.ActivityDisplayImages.ImagesDowaaActivity" />

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-4010171402649915~5502252008" />

        <activity android:name=".Adapter.PicassoDisplayImageAdapter" />
        <activity
            android:name=".ActivityProjects.ActivityDisplayImages.ImagesMoqAlaramActivity"
            android:label="المسجد الحرام" />
        <activity
            android:name=".ActivityProjects.ActivityDisplayImages.ImagesMoqNabaweActivity"
            android:label="المسجد النبوي" />
        <activity
            android:name=".ActivityProjects.ActivityDisplayImages.ImagesSaleheenActivity"
            android:label="أقوال الصالحين" />
        <activity
            android:name=".ActivityProjects.ActivityDisplayImages.ImagesAqsaActivity"
            android:label="المسجد الأقصى" />
        <activity
            android:name=".ActivityProjects.ActivityDisplayImages.ImagesOthersActivity"
            android:label="صور منوعة" />
        <activity
            android:name=".ActivityProjects.ActivityDisplayImages.ImagesRamadanActivity"
            android:label="رمضان كريم" />
        <activity android:name=".Activity.MainActivity" />
        <activity android:name=".ActivityProjects.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>